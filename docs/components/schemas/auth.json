{
    "authData": {
        "type": "object",
        "properties": {
            "token": {
                "type": "string",
                "example": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpZCI6MSwibmFtZSI6IkpvaG4gRG9lIiwiaWF0IjoxNTE2MjM5MDIyfQ.SflKxwRJSMeKKF2QT4fwpMeJf36POk6yJV_adQssw5c"
            }
        }
    },
    "authResponse": {
        "type": "object",
        "allOf": [
            {
                "$ref": "#/components/schemas/responseSuccess"
            }
        ],
        "properties": {
            "data": {
                "$ref": "#/components/schemas/authData"
            }
        }
    },
    "loginRequest": {
        "type": "object",
        "properties": {
            "user": {
                "type": "string"
            },
            "password": {
                "type": "string",
                "format": "password"
            }
        }
    },
    "registerRequest": {
        "type": "object",
        "properties": {
            "username": {
                "type": "string"
            },
            "name": {
                "type": "string"
            },
            "email": {
                "type": "string",
                "example": "finanzas@example.com"
            },
            "password": {
                "type": "string",
                "format": "password"
            },
            "phone": {
                "type": "string",
                "example": "+5712345678"
            }
        }
    }
}